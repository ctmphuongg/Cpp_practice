// Cao Thi Minh Phuong 11CT - 18
#include <iostream>
#include <cmath>
using namespace std;
int main () {
  double a, b, c;
  cin >> a >> b >> c;
  double d;
  d = b*b - 4*a*c; 
  if (a==0) {
    if (b==0 && c==0) {
      cout << "Phuong trinh co vo so nghiem";}
    if (b==0 && c!=0) {
      cout << "Phuong trinh vo nghiem";}
    if (b!=0) {
      cout << "x=" << c/b;
    }
  } else {
    if (d<0) {
      cout << "Phuong trinh vo nghiem";  
    } else if (d==0) {
      cout << "x=" << -b/(2*a);
    } else if (d>0) {
      cout << "x1=" << (-b+sqrt(d))/(2*a) << ", x2=" << (-b-sqrt(d))/(2*a);
    }
  }
  return 0;
} 
